  <xs:element name="log4net" type="log4net">
    <xs:key name="appenderNamesKey">
      <xs:selector xpath="./appender" />
      <xs:field xpath="@name" />
    </xs:key>
    <xs:keyref name="appenderExists" refer="appenderNamesKey">
      <xs:selector xpath=".//appender-ref"/>
      <xs:field xpath="@ref"/>
    </xs:keyref>
  </xs:element>
  <xs:complexType name="log4net">
